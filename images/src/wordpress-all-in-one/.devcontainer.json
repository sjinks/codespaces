{
    "x-build": {
        "name": "WordPress All-in-One",
        "image-name": "wordpress-all-in-one",
        "image-version": "2.1.0"
    },
    "build": {
        "dockerfile": "./Dockerfile",
        "context": "."
    },
    "forwardPorts": [80, 8025],
    "portsAttributes": {
        "80": {
            "label": "Application",
            "onAutoForward": "notify"
        },
        "8025": {
            "label": "Mailpit",
            "onAutoForward": "notify"
        }
    }
}
